{
  "name": "infinite-scrolling",
  "version": "0.2.5",
  "author": "tnfe",
  "license": "MIT",
  "description": "An infinite scrolling list of vue3, of course vue2 can also be used.",
  "keywords": [
    "infinite-list",
    "infinite-scroll",
    "vue3"
  ],
  "private": false,
  "main": "dist/infinite-scrolling.mjs",
  "exports": {
    ".": {
      "import": "./dist/infinite-scrolling.mjs",
      "require": "./dist/infinite-scrolling.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tnfe/infinite-scrolling"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "docs": "vite build --config vite.doc.config.js",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i"
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@better-scroll/pull-down": "^2.5.1",
    "@better-scroll/pull-up": "^2.5.1",
    "rollup-plugin-javascript-obfuscator": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "vite-plugin-compression": "^0.5.1"
  },
  "peerDependencies": {
    "@vue/composition-api": "1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/compiler-sfc": "^3.2.31",
    "element-plus": "^2.3.5",
    "sass": "latest",
    "typescript": "latest",
    "vite": "^4.3.5",
    "vite-plugin-css-injected-by-js": "^3.1.1",
    "vue": "^3.3.4",
    "vue-router": "^4.0.14"
  }
}
